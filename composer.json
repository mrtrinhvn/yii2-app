{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",

        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "*",

        "facebook/php-sdk-v4" : "4.0.*",
        "google/apiclient": "1.0.*@beta",
        "asofter/yii2-imperavi-redactor": "dev-master",
        "dropbox/dropbox-sdk": "1.1.*",

        "harrytang/yii2-dotdotdot": "*",
        "harrytang/yii2-core": "dev-master",
        "harrytang/yii2-gii": "dev-master",
        "harrytang/yii2-i18n": "dev-master",
        "harrytang/yii2-bootstrapsocial": "dev-master",
        "harrytang/yii2-admin": "dev-master",
        "harrytang/yii2-account": "dev-master",
        "harrytang/yii2-fineuploader": "dev-master",


        "harrytang/yii2-product": "dev-master",
        "harrytang/yii2-cms": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
